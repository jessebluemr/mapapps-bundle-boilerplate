{
	"Bundle-SymbolicName": "mapapps-bundle-boilerplate",
	"Bundle-Version": "0.0.1",
	"Bundle-Localization": [],
	"Require-Bundle": [{
		"name": "map",
		"version": "^3.3.0"
	}],
	"CSS-Themes-Extension": [{
		"name": "*",
		"files": ["./css/styles.css"]
	}],
	"layout-widgets": [{
		"widgetRole": "myWidget",
		"window": {
			"title": "A Window Title",
			"closable": false,
			"show": true,
			"marginBox": {
				"w": 250,
				"h": 210
			}
		}
	}],
	"Components": [{
		"name": "Widget",
		"provides": ["dijit.Widget"],
		"propertiesConstructor": true,
		"properties": {
			"widgetRole": "myWidget"
		}
	}]
}